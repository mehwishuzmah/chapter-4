#include<iostream>
using namespace std;
int main(){
	int temp,num,digit,rev=0;
	cout<<"enter a positive number";
	cin>>num;
	temp=num;
	while(num>0){
		digit=num%10;
		rev=(rev*10)+digit;
		num=num/10;
	}
	cout<<"the reverse of the numberis "<<rev<<"\n";
	if(temp==rev)
	cout<<"the number is palindrome";
	else
	cout<<"the number is not a palindrome";
	return 0;
}
